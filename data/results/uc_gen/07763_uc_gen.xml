<useCase id="UC1">
    <name>Import Json File</name>
    
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    
    <preconditions>
        <precondition>Json file exists</precondition>
    </preconditions>
    
    <mainSequence>
        <step id="S1">Inform JsonImporterUI to import a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user for information</step>
        <step id="S4">JsonImporterUI receives filename</step>
        <step id="S5">JsonImporterUI activates user interface</step>
        <step id="S6">JsonImporter creates</step>
        <step id="S7">JsonImporter triggers importer function</step>
        <step id="S8">JsonImporter parses warehousePlant data</step>
        <step id="S9">WarehouseBuilder creates warehouse</step>
        <step id="S10">WarehouseRepository saves warehouse</step>
        <step id="S11">AisleBuilder creates aisle</step>
        <step id="S12">AisleRepository saves aisle</step>
        <step id="S13">RowBuilder creates section</step>
        <step id="S14">SectionRepository saves section</step>
        <step id="S15">ShelfBuilder creates shelf</step>
        <step id="S16">ShelfRepository saves shelf</step>
        <step id="S17">AGVDockBuilder creates AGV dock</step>
        <step id="S18">AGVDockRepository saves AGV dock</step>
        <step id="S19">JsonImporter displays user information</step>
    </mainSequence>
</useCase>
