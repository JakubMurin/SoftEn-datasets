<useCase id="UC1">
    <name>Consultation Process</name>
    <actors>
        <actor>cliente</actor>
        <actor>secretaria</actor>
        <actor>veterinario</actor>
    </actors>
    <mainSequence>
        <step id="S1">cliente marcar consulta com secretaria</step>
        <step id="S2">secretaria confirmar consulta para cliente</step>
        <step id="S3">cliente informar dados pessoais para secretaria</step>
        <step id="S4">secretaria atualizar cadastro</step>
        <step id="S5">secretaria confirmar atualização para cliente</step>
        <step id="S6">cliente informar sintomas para veterinario</step>
        <step id="S7">veterinario registrar sintomas</step>
        <step id="S8">veterinario fornecer diagnóstico e plano de tratamento para cliente</step>
        <step id="S9">veterinario marcar exames</step>
        <step id="S10">veterinario solicitar exames para cliente</step>
        <step id="S11">veterinario finalizar tratamento</step>
        <step id="S12">cliente retornar para consulta com veterinario</step>
        <step id="S13">veterinario registrar pedido de exames</step>
        <step id="S14">veterinario fornecer resultados no histórico para cliente</step>
        <step id="S15">veterinario fornecer receitas para cliente</step>
    </mainSequence>
</useCase>
