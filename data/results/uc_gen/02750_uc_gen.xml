<useCase id="UC001">
    <name>Manage Applications</name>
    <actors>
        <actor>Developer</actor>
    </actors>
    <mainSequence>
        <step id="S1">Developer initiates fleet-service-inc request</step>
        <step id="S2">fleet-service-inc request is sent to fleet</step>
        <step id="S3">fleet processes the request</step>
        <step id="S4">fleet sends service/inc request to Application Stack</step>
        <step id="S5">Application Stack executes sails.helpers.incService()</step>
    </mainSequence>
</useCase>
