<useCase id="UC1">
    <name>Make a phone call</name>
    <actors>
        <actor>Caller</actor>
        <actor>Phone booth</actor>
        <actor>Receiver</actor>
    </actors>
    <mainSequence>
        <step id="S1">Caller adds money to the phone booth</step>
        <step id="S2">Caller uses the phone booth</step>
        <step id="S3">Phone booth initiates a call to the Receiver</step>
        <step id="S4">Receiver hangs up the call</step>
        <step id="S5">Receiver's call time ends</step>
        <step id="S6">Phone booth removes money</step>
        <step id="S7">Phone booth returns money to the Caller</step>
    </mainSequence>
</useCase>
