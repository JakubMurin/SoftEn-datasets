<useCase id="UC1">
    <name>Importing a Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides Filename</step>
        <step id="S5">JsonImporterController creates</step>
        <step id="S6">JsonImporter creates</step>
        <step id="S7">JsonImporter imports the file</step>
        <step id="S8">JsonImporter parses the warehouse</step>
        <step id="S9">WarehouseBuilder creates the warehouse</step>
        <step id="S10">Warehouse is saved in WarehouseRepository</step>
        <step id="S11">AisleBuilder creates an aisle</step>
        <step id="S12">Aisle is saved in AisleRepository</step>
        <step id="S13">SectionBuilder creates a section</step>
        <step id="S14">Section is saved in SectionRepository</step>
        <step id="S15">ShelfBuilder creates a shelf</step>
        <step id="S16">Shelf is saved in ShelfRepository</step>
        <step id="S17">AGVDockBuilder creates an AGV Dock</step>
        <step id="S18">AGV Dock is saved in AGVDockRepository</step>
        <step id="S19">UserInfo is displayed to WarehouseEmployee</step>
    </mainSequence>
</useCase>
