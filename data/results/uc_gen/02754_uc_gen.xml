<useCase id="UC001">
    <name>Manage Applications</name>
    <actors>
        <actor>Developer</actor>
    </actors>
    <mainSequence>
        <step id="S1">Developer selects fleet-service-inc</step>
        <step id="S2">fleet-service-inc communicates with fleet</step>
        <step id="S3">fleet-service-inc invokes service/inc in Application Stack</step>
        <step id="S4">service/inc executes sails.helpers.incService()</step>
    </mainSequence>
</useCase>
