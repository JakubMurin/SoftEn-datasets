<useCase id="UC1">
    <name>Update Project Information</name>
    <actors>
        <actor>Client</actor>
        <actor>Website</actor>
        <actor>UpdateProjectActivity</actor>
        <actor>Projects Database</actor>
    </actors>
    <mainSequence>
        <step id="S1">Client submits update form</step>
        <step id="S2">Website sends update project request</step>
        <step id="S3">UpdateProjectActivity saves updates in Projects Database</step>
        <step id="S4">UpdateProjectActivity retrieves all projects from Projects Database</step>
        <step id="S5">Website presents a list of all projects to the Client</step>
    </mainSequence>
</useCase>
