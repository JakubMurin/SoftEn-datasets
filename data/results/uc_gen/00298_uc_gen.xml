<useCase id="UC1">
    <name>Log-out User</name>
    <actors>
        <actor>Logged-in User</actor>
    </actors>
    <preconditions>
        <precondition>User is logged in.</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">User clicks on "log-out" button.</step>
        <step id="S2">GUI resets current user data.</step>
        <step id="S3">Redirect user to the welcome screen.</step>
    </mainSequence>
</useCase>
