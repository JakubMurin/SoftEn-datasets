<useCase id="UC1">
    <name>Access Environment and View Results</name>
    <actors>
        <actor>User</actor>
        <actor>Environment Page</actor>
        <actor>Search Service</actor>
        <actor>Database</actor>
        <actor>Show Result Page</actor>
    </actors>
    <preconditions>
        <precondition>User has access to the system</precondition>
    </preconditions>
    <successEndConditions>
        <condition>User successfully views the results</condition>
    </successEndConditions>
    <trigger>User accesses the environment page</trigger>
    <mainSequence>
        <step id="S1">User accesses the environment page</step>
        <step id="S2">Environment Page initiates search</step>
        <step id="S3">Search Service queries for task or environment</step>
        <step id="S4">Database returns task or environment data</step>
        <step id="S5">Search Service redirects to show result page</step>
        <step id="S6">User views the results</step>
        <step id="S7">User accesses task or environment</step>
    </mainSequence>
</useCase>
