<useCase id="UC1">
    <name>Main Task Execution</name>
    <actors>
        <actor>User</actor>
        <actor>ReWOOAgent</actor>
        <actor>ReasonLLM</actor>
        <actor>WorldLLM</actor>
        <actor>ObserveLLM</actor>
        <actor>OutcomeLLM</actor>
    </actors>
    <mainSequence>
        <step id="S1">User runs the input on the ReWOOAgent</step>
        <step id="S2">ReWOOAgent reasons about the action</step>
        <step id="S3">ReWOOAgent simulates the world</step>
        <step id="S4">ReWOOAgent observes the effects</step>
        <step id="S5">ReWOOAgent determines the outcome</step>
        <step id="S6">ReWOOAgent provides the final result to the User</step>
    </mainSequence>
</useCase>
