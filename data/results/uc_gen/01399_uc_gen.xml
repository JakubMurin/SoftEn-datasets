<useCase id="UC1">
    <name>Delete Other Service</name>
    <actors>
        <actor>Admin</actor>
    </actors>
    <preconditions>
        <precondition>Admin is logged in</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">Admin requests to delete an existing other service</step>
        <step id="S2">UI receives the request</step>
        <step id="S3">Router processes the request</step>
        <step id="S4">Controller shows delete confirmation</step>
        <step id="S5">Repository fetches service details</step>
        <step id="S6">OtherServiceModel fetches service by ID</step>
        <step id="S7">Database queries service by ID</step>
        <step id="S8">OtherServiceModel receives service data</step>
        <step id="S9">Repository receives service data</step>
        <step id="S10">Controller displays delete confirmation form</step>
        <step id="S11">Admin confirms delete request</step>
        <step id="S12">UI receives confirmation</step>
        <step id="S13">Router processes delete request</step>
        <step id="S14">Controller deletes other service</step>
        <step id="S15">Repository deletes service</step>
        <step id="S16">OtherServiceModel deletes service from database</step>
        <step id="S17">Database confirms service deletion</step>
        <step id="S18">Repository confirms service deletion</step>
        <step id="S19">Controller sends 200 OK response</step>
        <step id="S20">Router receives 200 OK response</step>
        <step id="S21">UI displays success message</step>
    </mainSequence>
</useCase>
