<useCase id="UC1">
    <name>Authenticate User</name>
    <actors>
        <actor>Bob</actor>
        <actor>Alice</actor>
        <actor>I have a really long name</actor>
    </actors>
    <mainSequence>
        <step id="S1">Bob sends an Authentication Request</step>
        <step id="S2">Alice receives the Authentication Request</step>
        <step id="S3">Alice sends an Authentication Response</step>
        <step id="S4">Bob receives the Authentication Response</step>
        <step id="S5">Bob logs the transaction</step>
    </mainSequence>
</useCase>
