<useCase id="UC1">
    <name>Import Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <preconditions>
        <precondition>Warehouse Employee has access to JsonImporterUI</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides filename</step>
        <step id="S5">JsonImporter creates</step>
        <step id="S6">JsonImporter importer(filename)</step>
        <step id="S7">JsonImporter parses warehousePlant</step>
        <step id="S8">WarehouseBuilder creates</step>
        <step id="S9">Warehouse is created</step>
        <step id="S10">Warehouse is saved in WarehouseRepository</step>
        <step id="S11">AisleBuilder creates</step>
        <step id="S12">Aisle is created</step>
        <step id="S13">Aisle is saved in AisleRepository</step>
        <step id="S14">SectionBuilder creates</step>
        <step id="S15">Section is created</step>
        <step id="S16">Section is saved in SectionRepository</step>
        <step id="S17">ShelfBuilder creates</step>
        <step id="S18">Shelf is created</step>
        <step id="S19">Shelf is saved in ShelfRepository</step>
        <step id="S20">AGVDockBuilder creates</step>
        <step id="S21">AGVDock is created</step>
        <step id="S22">AGVDock is saved in AGVDockRepository</step>
        <step id="S23">UserInfo is displayed</step>
    </mainSequence>
</useCase>
