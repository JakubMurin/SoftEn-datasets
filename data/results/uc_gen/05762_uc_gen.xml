<useCase id="UC1">
    <name>Import Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides the Filename</step>
        <step id="S5">JsonImporterController creates</step>
        <step id="S6">JsonImporterController calls jsonImporter with filename</step>
        <step id="S7">JsonImporter creates</step>
        <step id="S8">JsonImporter calls importer with filename</step>
        <step id="S9">JsonImporter parses warehousePlant</step>
        <step id="S10">WarehouseBuilder creates</step>
        <step id="S11">WarehouseBuilder creates warehouse</step>
        <step id="S12">WarehouseBuilder provides warehouse to JsonImporter</step>
        <step id="S13">WarehouseRepository saves warehouse</step>
        <step id="S14">AisleBuilder creates</step>
        <step id="S15">AisleBuilder creates aisle</step>
        <step id="S16">AisleBuilder provides aisle to JsonImporter</step>
        <step id="S17">AisleRepository saves aisle</step>
        <step id="S18">SectionBuilder creates</step>
        <step id="S19">SectionBuilder creates section</step>
        <step id="S20">SectionBuilder provides section to JsonImporter</step>
        <step id="S21">SectionRepository saves section</step>
        <step id="S22">ShelfBuilder creates</step>
        <step id="S23">ShelfBuilder creates shelf</step>
        <step id="S24">ShelfBuilder provides shelf to JsonImporter</step>
        <step id="S25">ShelfRepository saves shelf</step>
        <step id="S26">AGVDockBuilder creates</step>
        <step id="S27">AGVDockBuilder creates AGVDock</step>
        <step id="S28">AGVDockBuilder provides AGVDock to JsonImporter</step>
        <step id="S29">AGVDockRepository saves AGVDock</step>
        <step id="S30">JsonImporter displays user information</step>
    </mainSequence>
</useCase>
