<useCase id="UC1">
    <name>Log-out</name>
    <actors>
        <actor>Logged-in User</actor>
    </actors>
    <mainSequence>
        <step id="S1">The Logged-in User clicks on "log-out"</step>
        <step id="S2">The GUI Main Screen resets the current user data</step>
        <step id="S3">The system returns OK</step>
        <step id="S4">The system redirects to the welcome screen</step>
    </mainSequence>
</useCase>
