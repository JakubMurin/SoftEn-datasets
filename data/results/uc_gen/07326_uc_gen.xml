<useCase id="UC001">
    <name>Display Class Attributes</name>
    <actors>
        <actor>User</actor>
        <actor>ViewAllClasses</actor>
        <actor>ModelClass</actor>
    </actors>
    <mainSequence>
        <step id="S1">User requests the display of attributes</step>
        <step id="S2">ViewAllClasses receives the request</step>
        <step id="S3">ViewAllClasses calls ModelClass's getAttributes()</step>
        <step id="S4">ModelClass returns ArrayList of attributes to ViewAllClasses</step>
        <step id="S5">ViewAllClasses renders the attributes in the UI</step>
    </mainSequence>
</useCase>
