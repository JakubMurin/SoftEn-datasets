<useCase id="UC1">
    <name>Achieve Faculty Member Search Information</name>
    <actors>
        <actor>FacultyMember</actor>
    </actors>
    <mainSequence>
        <step id="S1">Faculty Member enters login credentials</step>
        <step id="S2">Credentials are validated</step>
        <step id="S3">Faculty Member accesses the search interface</step>
        <step id="S4">Search options are displayed</step>
        <step id="S5">Faculty Member inputs search criteria for collections</step>
        <step id="S6">Database queried with the search criteria</step>
        <step id="S7">Retrieved list of matching collections displayed</step>
        <step id="S8">Faculty Member selects desired collection</step>
        <step id="S9">Selected collection details are requested</step>
        <step id="S10">Collection details are viewed by the Faculty Member</step>
    </mainSequence>
</useCase>
