<useCase id="UC1">
    <name>Upload Object with Quota Validation</name>
    <actors>
        <actor>Client</actor>
        <actor>RPC Server</actor>
        <actor>Object Server</actor>
    </actors>
    <mainSequence>
        <step id="S1">Client creates object upload request.</step>
        <step id="S2">RPC Server forwards the request to Object Server.</step>
        <step id="S3">Object Server checks the quota.</step>
    </mainSequence>
</useCase>
