<useCase id="UC1">
    <name>Searching Staff Profiles</name>
    <actors>
        <actor>Admin</actor>
        <actor>HealthCare System</actor>
    </actors>
    <preconditions>
        <precondition>Admin access privileges</precondition>
    </preconditions>
    <successEndConditions>
        <condition>Staff profiles successfully retrieved</condition>
    </successEndConditions>
    <failureEndConditions>
        <condition>No staff profiles found</condition>   
    </failureEndConditions>
    <mainSequence>
        <step id="S1">Admin requests to search for staff profiles by listing name, specialization, or email.</step>
        <step id="S2">HealthCare System receives the search request.</step>
        <step id="S3">HealthCare System lists staff profiles.</step>
    </mainSequence>
</useCase>
