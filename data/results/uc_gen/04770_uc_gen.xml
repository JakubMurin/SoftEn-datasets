<useCase id="UC1">
    <name>Import Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides the filename</step>
        <step id="S5">JsonImporterUI receives the filename</step>
        <step id="S6">JsonImporterController creates JsonImporter</step>
        <step id="S7">JsonImporterController calls jsonImporter with filename</step>
        <step id="S8">JsonImporter creates</step>
        <step id="S9">JsonImporter imports the warehouse from the Json file</step>
        <step id="S10">WarehouseBuilder creates a warehouse</step>
        <step id="S11">WarehouseBuilder creates a warehouse</step>
        <step id="S12">WarehouseBuilder saves the warehouse to WarehouseRepository</step>
        <step id="S13">AisleBuilder creates an aisle</step>
        <step id="S14">AisleBuilder saves the aisle to AisleRepository</step>
        <step id="S15">SectionBuilder creates a section</step>
        <step id="S16">SectionBuilder saves the section to SectionRepository</step>
        <step id="S17">ShelfBuilder creates a shelf</step>
        <step id="S18">ShelfBuilder saves the shelf to ShelfRepository</step>
        <step id="S19">AGVDockBuilder creates an AGV dock</step>
        <step id="S20">AGVDockBuilder saves the AGV dock to AGVDockRepository</step>
        <step id="S21">JsonImporter displays user information</step>
    </mainSequence>
</useCase>
