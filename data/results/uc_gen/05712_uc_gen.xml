<useCase id="UC1">
    <name>Manage-Application</name>
    <actors>
        <actor>actorName</actor>
    </actors>
    <mainSequence>
        <step id="S1">Actor "actorName" initiates Edgeville-ai-deploy.</step>
        <step id="S2">CLI receives Edgeville-ai-deploy request.</step>
        <step id="S3">CLI sends ai/deploy request to Edgeville.</step>
        <step id="S4">Actor "actorName" sends ai/deploy request to Web.</step>
        <step id="S5">Web sends ai/deploy request to Edgeville.</step>
    </mainSequence>
</useCase>
