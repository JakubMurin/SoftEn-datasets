<useCase id="UC1">
  <name>Get Partial Stats</name>
  <actors>
    <actor>Client</actor>
    <actor>RS</actor>
    <actor>SA</actor>
  </actors>
  <mainSequence>
    <step id="S1">Client requests get_partial_stats</step>
    <step id="S2">RS forwards the request to SA</step>
    <step id="S3">SA performs get_partial_systems_stats</step>
    <step id="S4">SA performs _partial_buckets_info</step>
    <step id="S5">SA sends stats_payload to Client</step>
  </mainSequence>
</useCase>
