<useCase id="UC001">
    <name>View All Classes</name>
    <actors>
        <actor>User</actor>
        <actor>ViewAllClasses</actor>
        <actor>ModelClass</actor>
        <actor>Attribute</actor>
    </actors>
    <preconditions>
        <precondition>User requests display of attributes</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">User requests display of attributes</step>
        <step id="S2">ViewAllClasses calls getAttributes() from ModelClass</step>
        <step id="S3">ModelClass returns ArrayList of Attribute</step>
        <step id="S4">For each attribute: Retrieve attribute details</step>
        <step id="S5">Attribute returns attribute properties</step>
        <step id="S6">Process attributes for display</step>
        <step id="S7">Render attributes in the UI</step>
        <step id="S8">Display complete</step>
    </mainSequence>
</useCase>
