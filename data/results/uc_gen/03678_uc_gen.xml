<useCase id="UC1">
    <name>Access Environment</name>
    <actors>
        <actor>User</actor>
    </actors>
    <mainSequence>
        <step id="S1">User accesses the environment page</step>
        <step id="S2">Environment page initiates the search</step>
        <step id="S3">Search service queries for a task or environment</step>
