<useCase id="UC001">
    <name>Conflict Resolution</name>
    <actors>
        <actor>User Device</actor>
        <actor>NotificationService</actor>
        <actor>MQTTServer</actor>
    </actors>
    <mainSequence>
        <step id="S1">Receive Conflict Event from MQTTServer</step>
        <step id="S2">Process Conflict Event in NotificationService</step>
        <step id="S3">Send Conflict Notification to User Device</step>
    </mainSequence>
</useCase>
