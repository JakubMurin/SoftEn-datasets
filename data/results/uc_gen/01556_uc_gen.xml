<useCase id="UC1">
    <name>Make teleport gate result</name>
    <actors>
        <actor>Tester</actor>
        <actor>recipe</actor>
        <actor>s</actor>
        <actor>inventory</actor>
        <actor>gate1</actor>
        <actor>gate2</actor>
    </actors>
    <mainSequence>
        <step id="S1">Tester makes the result with s</step>
        <step id="S2">Recipe creates TeleportGateItem with ID for gate1</step>
        <step id="S3">Recipe creates TeleportGateItem with ID for gate2</step>
        <step id="S4">Recipe inserts gate1 into inventory</step>
        <step id="S5">Recipe confirms successful insertion of gate1</step>
        <step id="S6">Recipe inserts gate2 into inventory</step>
        <step id="S7">Recipe confirms successful insertion of gate2</step>
        <step id="S8">Recipe notifies Tester</step>
    </mainSequence>
</useCase>
