<useCase id="UC01">
	<name>Admin adds a new other service</name>
	<actors>
		<actor>Admin</actor>
	</actors>
	<mainSequence>
		<step id="S1">Admin requests to add a new other service</step>
		<step id="S2">Admin submits new service data</step>
		<step id="S3">UI displays add service form</step>
		<step id="S4">Controller adds other service</step>
		<step id="S5">Repository saves new service</step>
		<step id="S6">OtherServiceModel creates new service</step>
		<step id="S7">DB inserts new service into the database</step>
		<step id="S8">User interface displays success message</step>
	</mainSequence>
</useCase>
