<useCase id="UC_login_start_quiz_view_results_practice">
  <name>Login, Start Quiz, View Results, and Practice Quiz</name>
  <actors>
    <actor>Student</actor>
    <actor>HomePage</actor>
    <actor>LoginPage</actor>
    <actor>DashboardPage</actor>
    <actor>QuizPage</actor>
    <actor>APIHandlers</actor>
    <actor>SharedFiles</actor>
  </actors>
  <mainSequence>
    <step id="S1">Student accesses homepage.</step>
    <step id="S2">Homepage redirects to login page.</step>
    <step id="S3">Student enters ID on login page.</step>
    <step id="S4">Login successful, redirected to dashboard.</step>
    <step id="S5">Dashboard accessed, "Start Quiz" clicked.</step>
    <step id="S6">Quiz navigation to specific URL.</step>
    <step id="S7">API reads quiz data from files.</step>
    <step id="S8">Submission of quiz answers completed.</step>
    <step id="S9">Result viewed by student on dashboard.</step>
    <step id="S10">Practice Quiz initiated by student on dashboard.</step>
    <step id="S11">Practice quiz data retrieved from API.</step>
  </mainSequence>
</useCase>
