<useCase id="UC1">
    <name>Import Warehouse Data</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides the Filename</step>
        <step id="S5">JsonImporterController creates</step>
        <step id="S6">JsonImporterController invokes JsonImporter with filename</step>
        <step id="S7">JsonImporter creates</step>
        <step id="S8">JsonImporter parses warehouse information</step>
        <step id="S9">WarehouseBuilder creates a new Warehouse</step>
        <step id="S10">WarehouseBuilder creates new Warehouse object</step>
        <step id="S11">JsonImporter receives the new Warehouse</step>
        <step id="S12">WarehouseRepository saves the new Warehouse</step>
        <step id="S13">AisleBuilder creates a new Aisle</step>
        <step id="S14">AisleBuilder creates new Aisle object</step>
        <step id="S15">JsonImporter receives the new Aisle</step>
        <step id="S16">AisleRepository saves the new Aisle</step>
        <step id="S17">SectionBuilder creates a new Section</step>
        <step id="S18">SectionBuilder creates new Section object</step>
        <step id="S19">JsonImporter receives the new Section</step>
        <step id="S20">SectionRepository saves the new Section</step>
        <step id="S21">ShelfBuilder creates a new Shelf</step>
        <step id="S22">ShelfBuilder creates new Shelf object</step>
        <step id="S23">JsonImporter receives the new Shelf</step>
        <step id="S24">ShelfRepository saves the new Shelf</step>
        <step id="S25">AGVDockBuilder creates a new AGV Dock</step>
        <step id="S26">AGVDockBuilder creates new AGV Dock object</step>
        <step id="S27">JsonImporter receives the new AGV Dock</step>
        <step id="S28">AGVDockRepository saves the new AGV Dock</step>
        <step id="S29">JsonImporter completes processing</step>
        <step id="S30">JsonImporterUI displays user information</step>
    </mainSequence>
</useCase>
