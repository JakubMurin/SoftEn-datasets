<useCase id="UC1">
    <name>Import Json File and Build Warehouse Structure</name>
    
    <actors>
        <actor>Warehouse Employee</actor>
    </actors>
    
    <mainSequence>
        <step id="S1">Initiate Json file import</step>
        <step id="S2">Prompt user for information</step>
        <step id="S3">Receive filename from user</step>
        <step id="S4">Call JsonImporter to create</step>
        <step id="S5">Parse Warehouse data from Json</step>
        <step id="S6">Save imported Warehouse to Repository</step>
        <step id="S7">Build Aisle from Warehouse data</step>
        <step id="S8">Save imported Aisle to Repository</step>
        <step id="S9">Build Section from Aisle data</step>
        <step id="S10">Save imported Section to Repository</step>
        <step id="S11">Build Shelf from Section data</step>
        <step id="S12">Save imported Shelf to Repository</step>
        <step id="S13">Build AGV Dock from Warehouse data</step>
        <step id="S14">Save imported AGV Dock to Repository</step>
        <step id="S15">Display the user information</step>
    </mainSequence>
</useCase>
