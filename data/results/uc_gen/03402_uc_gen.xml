<useCase id="UC1">
    <name>Generate Hash</name>
    <actors>
        <actor>User</actor>
        <actor>MD4</actor>
    </actors>
    <mainSequence>
        <step id="S1">User hashes the input value.</step>
        <step id="S2">MD4 initializes the message size.</step>
        <step id="S3">MD4 preprocesses the input value.</step>
        <step id="S4">MD4 processes the input.</step>
        <step id="S5">MD4 returns the hashed value to the User.</step>
    </mainSequence>
</useCase>
