        <step id="S26">RowBuilder creates the section</step>
        <step id="S27">JsonImporter receives the section</step>
        <step id="S28">RowBuilder saves the section to SectionRepository</step>
        <step id="S29">ShelfBuilder creates</step>
        <step id="S30">ShelfBuilder creates a shelf</step>
        <step id="S31">ShelfBuilder creates the shelf</step>
        <step id="S32">JsonImporter receives the shelf</step>
        <step id="S33">ShelfBuilder saves the shelf to ShelfRepository</step>
        <step id="S34">AGVDockBuilder creates</step>
        <step id="S35">AGVDockBuilder creates an AGV dock</step>
        <step id="S36">AGVDockBuilder creates the AGV dock</step>
        <step id="S37">JsonImporter receives the AGV dock</step>
        <step id="S38">AGVDockBuilder saves the AGV dock to AGVDockRepository</step>
        <step id="S39">JsonImporter deactivates</step>
        <step id="S40">JsonImporterController deactivates</step>
        <step id="S41">JsonImporterUI displays user information</step>
    </mainSequence>
