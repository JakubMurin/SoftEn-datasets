        <step id="S8">JsonImporterUI asks for Filename</step>
        <step id="S9">JsonImporterUI activates</step>
        <step id="S10">JsonImporterController calls jsonImporter with filename</step>
        <step id="S11">JsonImporter creates</step>
        <step id="S12">JsonImporter imports the file</step>
        <step id="S13">JsonImporter parses the warehouse from the file</step>
        <step id="S14">WarehouseBuilder creates</step>
        <step id="S15">WarehouseBuilder creates a warehouse</step>
