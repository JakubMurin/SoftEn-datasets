<useCase id="scanner_job">
    <name>Scanner Job</name>
    <actors>
        <actor>admin_user</actor>
        <actor>admin</actor>
        <actor>cc</actor>
        <actor>sc</actor>
        <actor>explorer</actor>
        <actor>worker</actor>
        <actor>exporter</actor>
        <actor>ec</actor>
        <actor>rc</actor>
        <actor>report_user</actor>
    </actors>
    <mainSequence>
        <step id="S1">Start Scanner job</step>
        <step id="S2">Scanner job started</step>
        <step id="S3">Exploration stage</step>
        <step id="S4">Worker stage</step>
        <step id="S5">Exporter Stage</step>
        <step id="S6">Checkup Collector</step>
        <step id="S7">Status Collector</step>
        <step id="S8">Event Collector</step>
        <step id="S9">Result Collector</step>
        <step id="S10">Scanner job complete</step>
    </mainSequence>
</useCase>
