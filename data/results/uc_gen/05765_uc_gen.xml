<useCase id="UC1">
    <name>Import Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides Filename</step>
        <step id="S5">JsonImporterUI activates</step>
        <step id="S6">JsonImporterController creates</step>
        <step id="S7">JsonImporterUI deactivates</step>
        <step id="S8">JsonImporterUI asks for Filename</step>
        <step id="S9">JsonImporterUI activates</step>
        <step id="S10">JsonImporterController calls jsonImporter with filename</step>
        <step id="S11">JsonImporter creates</step>
        <step id="S12">JsonImporter imports the file</step>
        <step id="S13">JsonImporter parses the warehouse from the file</step>
        <step id="S14">WarehouseBuilder creates</step>
        <step id="S15">WarehouseBuilder creates a warehouse</step>
        <step id="S16">WarehouseBuilder assigns the warehouse to JsonImporter</step>
        <step id="S17">JsonImporter saves the warehouse to WarehouseRepository</step>
        <step id="S18">AisleBuilder creates</step>
        <step id="S19">AisleBuilder creates an aisle</step>
        <step id="S20">AisleBuilder assigns the aisle to JsonImporter</step>
        <step id="S21">JsonImporter saves the aisle to AisleRepository</step>
        <step id="S22">RowBuilder creates</step>
        <step id="S23">RowBuilder creates a section</step>
        <step id="S24">RowBuilder assigns the section to JsonImporter</step>
        <step id="S25">JsonImporter saves the section to SectionRepository</step>
        <step id="S26">ShelfBuilder creates</step>
        <step id="S27">ShelfBuilder creates a shelf</step>
        <step id="S28">ShelfBuilder assigns the shelf to JsonImporter</step>
        <step id="S29">JsonImporter saves the shelf to ShelfRepository</step>
        <step id="S30">AGVDockBuilder creates</step>
        <step id="S31">AGVDockBuilder creates an AGV dock</step>
        <step id="S32">AGVDockBuilder assigns the AGV dock to JsonImporter</step>
        <step id="S33">JsonImporter saves the AGV dock to AGVDockRepository</step>
        <step id="S34">JsonImporter deactivates</step>
        <step id="S35">JsonImporterController deactivates</step>
        <step id="S36">JsonImporterUI displays user information</step>
    </mainSequence>
</useCase>
