<useCase id="UC1">
    <name>Submit Question</name>
    <actors>
        <actor>User</actor>
        <actor>:View</actor>
        <actor>:Route</actor>
        <actor>:views.py</actor>
        <actor>:validate</actor>
        <actor>:Questions</actor>
        <actor>:Model</actor>
        <actor>:AdaptiveTesting</actor>
        <actor>:database</actor>
    </actors>
    <mainSequence>
        <step id="S1">User clicks "Submit" Question</step>
        <step id="S2">View executes "Submit Question"</step>
        <step id="S3">Route executes quiz()</step>
        <step id="S4">Views validate on submit</step>
        <step id="S5">Questions submit response</step>
        <step id="S6">Model gets student category</step>
        <step id="S7">Database saves data to file</step>
        <step id="S8">Questions get item responses</step>
        <step id="S9">Model corrects responses</step>
        <step id="S10">Views show result</step>
        <step id="S11">Route directs to /result</step>
        <step id="S12">View displays result.html</step>
    </mainSequence>
</useCase>
