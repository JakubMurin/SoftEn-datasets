<useCase id="1">
    <name>Execute Proposal</name>
    <actors>
        <actor>Alice_novice</actor>
    </actors>
    <mainSequence>
        <step id="S1">Alice_novice creates grants treasury</step>
        <step id="S2">Alice_novice receives treasury address</step>
        <step id="S3">Alice_novice creates forum proposal</step>
        <step id="S4">Forum proposal creation activated</step>
        <step id="S5">Community gets list of proposals from forum</step>
        <step id="S6">Community provides positive/negative feedback on proposals</step>
        <step id="S7">Alice_novice retrieves feedback</step>
        <step id="S8">Forum proposal creation deactivated</step>
        <step id="S9">Community delegates votes</step>
        <step id="S10">Alice_novice submits proposal to Snapshot</step>
        <step id="S11">TokenHolders_novice get list of proposals from Snapshot</step>
        <step id="S12">TokenHolders_novice votes on open proposals</step>
        <step id="S13">TokenHolders_novice confirms vote</step>
        <step id="S14">BitDAOAdmins_expert allows funds transfer</step>
        <step id="S15">BitDAOTreasury transfers funds to grants treasury</step>
        <step id="S16">Transfer confirmation received</step>
        <step id="S17">Alice_novice executes proposal</step>
    </mainSequence>
</useCase>
