<useCase id="UC1">
    <name>Take Quiz</name>
    <actors>
        <actor>User</actor>
    </actors>
    <mainSequence>
        <step id="S1">User takes the quiz</step>
        <step id="S2">Quiz is submitted to Questions</step>
        <step id="S3">Questions are checked by Answers</step>
        <step id="S4">Result is sent back to Questions</step>
        <step id="S5">Quiz results are sent back to User</step>
    </mainSequence>
</useCase>
