<useCase id="UC001">
    <name>Nametag Creation for Campers</name>
    <actors>
        <actor>Administrator</actor>
        <actor>Camp Management System</actor>
        <actor>Camper Profile Database</actor>
        <actor>Nametag Generator</actor>
        <actor>Printer</actor>
    </actors>
    <mainSequence>
        <step id="S1">Administrator selects "Generate Nametags"</step>
        <step id="S2">CMS retrieves all Camper Profiles</step>
        <step id="S3">CPD provides Camper Details</step>
        <step id="S4">CMS sends Camper Details to NG</step>
        <step id="S5">NG generates Nametags</step>
        <step id="S6">NG sends Nametags to CMS</step>
        <step id="S7">CMS sends Nametags for Printing to P</step>
        <step id="S8">P acknowledges Printing to CMS</step>
        <step id="S9">CMS confirms and provides Status to Administrator</step>
    </mainSequence>
</useCase>
