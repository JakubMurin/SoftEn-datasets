<useCase id="UC001">
    <name>Perform Online Banking Transactions</name>
    <actors>
        <actor>USER</actor>
    </actors>
    <mainSequence>
        <step id="S1">USER adds a bank account.</step>
        <step id="S2">USER initiates a money transfer.</step>
        <step id="S3">USER consults bank data for the transfer.</step>
        <step id="S4">USER views transaction history.</step>
    </mainSequence>
</useCase>
