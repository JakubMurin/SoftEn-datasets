<useCase id="UC1">
    <name>Attend Event</name>
    <actors>
        <actor>Entrant</actor>
        <actor>Ticket</actor>
        <actor>Member</actor>
    </actors>
    <mainSequence>
        <step id="S1">Entrant attends the event by requesting to Attend Event.</step>
        <step id="S2">Ticket receives Attend Event Request.</step>
        <step id="S3">Ticket initiates Create Member Request.</step>
        <step id="S4">Member creates a new member.</step>
        <step id="S5">Member responds to the Create Member Request.</step>
        <step id="S6">Ticket creates a ticket for the event.</step>
        <step id="S7">Entrant receives the Attend Event Response.</step>
    </mainSequence>
</useCase>
