<useCase id="UC001">
    <name>Review and Document Application Consistency</name>
    <actors>
        <actor>Planning Staff</actor>
        <actor>User Authentication</actor>
        <actor>Staff Dashboard</actor>
        <actor>Application Database</actor>
        <actor>Compliance Checking Tool</actor>
        <actor>Comprehensive Plan Database</actor>
        <actor>Review Notes Interface</actor>
        <actor>Notification Service</actor>
    </actors>
    <preconditions>
        <precondition>Planning Staff is logged in</precondition>
        <precondition>Applications are available for review</precondition>
    </preconditions>
    <trigger>Planning Staff initiates review process</trigger>
    <mainSequence>
        <step id="S1">Planning Staff logs into the system</step>
        <step id="S2">Planning Staff accesses the staff dashboard</step>
        <step id="S3">Planning Staff selects an application for review</step>
        <step id="S4">Planning Staff reviews application consistency using the Compliance Checking Tool</step>
        <step id="S5">Planning Staff documents the review in the Review Notes Interface</step>
        <step id="S6">Notification Service sends a notification to the applicant</step>
    </mainSequence>
</useCase>
