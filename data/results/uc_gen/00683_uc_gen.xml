<useCase id="UC1">
    <name>View Blog List</name>
    <actors>
        <actor>Admin</actor>
    </actors>
    <preconditions>
        <precondition>Admin is logged in</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">Admin requests to view the list of blog posts</step>
        <step id="S2">System verifies the request</step>
        <step id="S3">System retrieves the list of blog posts</step>
        <step id="S4">System displays the list of blog posts to Admin</step>
    </mainSequence>
</useCase>
