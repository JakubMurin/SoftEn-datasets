<useCase id="UC1">
    <name>User accesses environment and views search results</name>
    <actors>
        <actor>User</actor>
        <actor>Environment Page</actor>
        <actor>Search Service</actor>
        <actor>Database</actor>
        <actor>Show Result Page</actor>
    </actors>
    <mainSequence>
        <step id="S1">User accesses environment</step>
        <step id="S2">Environment Page initiates search</step>
        <step id="S3">Search Service queries database for task</step>
        <step id="S4">Database returns task data</step>
        <step id="S5">Search Service redirects to show result page</step>
        <step id="S6">User views search results</step>
        <step id="S7">Result Page accesses task or environment</step>
    </mainSequence>
</useCase>
