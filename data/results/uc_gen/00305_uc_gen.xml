<useCase id="UC001">
    <name>Log-out Feature</name>
    <actors>
        <actor>Logged-in User</actor>
    </actors>
    <mainSequence>
        <step id="S1">User logs out</step>
        <step id="S2">Logged-in User clicks on "log-out"</step>
        <step id="S3">GUI Main Screen redirects to user store</step>
        <step id="S4">GUI Current User Store resets current user data</step>
        <step id="S5">Return OK</step>
    </mainSequence>
</useCase>
