<useCase id="001">
    <name>Collaboration and Feedback Phase</name>
    <actors>
        <actor>NSF Employee</actor>
        <actor>Design Team</actor>
    </actors>
    <mainSequence>
        <step id="S1">Request insights and research on users</step>
        <step id="S2">Gather and analyze data</step>
        <step id="S3">Provide user insights</step>
        <step id="S4">Create preliminary design directions</step>
        <step id="S5">Present design concepts</step>
        <step id="S6">Provide feedback</step>
        <step id="S7">Share design directions and gather feedback</step>
        <step id="S8">Review design directions</step>
        <step id="S9">Provide feedback on designs</step>
        <step id="S10">Refine design based on feedback (Loop)</step>
        <step id="S11">Establish final brand guidelines</step>
        <step id="S12">Communicate brand guidelines</step>
        <step id="S13">Refer to guidelines during project</step>
    </mainSequence>
</useCase>
