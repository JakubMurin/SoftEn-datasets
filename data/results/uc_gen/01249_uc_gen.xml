<useCase id="UC1">
    <name>Turn On LED</name>
    <actors>
        <actor>Led</actor>
    </actors>
    <mainSequence>
        <step id="S1">Arduino calls Main's setup() method</step>
        <step id="S2">Main initializes the FSM context</step>
        <step id="S3">Main transitions to OnState in the FSM</step>
        <step id="S4">OnState executes onEnter() method</step>
        <step id="S5">OnState turns on the LED</step>
    </mainSequence>
</useCase>
