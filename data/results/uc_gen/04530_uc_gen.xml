<useCase id="1">
    <name>Execute Proposal</name>
    <actors>
        <actor>Alice_novice</actor>
        <actor>GrantsTreasury</actor>
    </actors>
    <mainSequence>
        <step id="S1">Alice_novice creates grants treasury</step>
        <step id="S2">GrantsTreasury provides treasury address</step>
        <step id="S3">Alice_novice creates a forum proposal</step>
        <step id="S4">Community_novice lists proposals from the forum</step>
        <step id="S5">Community_novice puts positive/negative feedback on the forum</step>
        <step id="S6">Alice_novice receives positive/negative feedback from the forum</step>
        <step id="S7">Community_novice delegates votes</step>
        <step id="S8">Alice_novice submits a treasury role proposal to BitDAO Governance</step>
        <step id="S9">TokenHolders_novice vote on the treasury role proposal at BitDAO Governance</step>
        <step id="S10">BitDAOAdmins_expert checks Alice's proposal result</step>
        <step id="S11">BitDAOAdmins_expert executes the proposal</step>
        <step id="S12">BitDaoGovernanceV2 transfers funds to GrantsTreasury</step>
        <step id="S13">GrantsTreasury confirms the funds transfer</step>
        <step id="S14">Alice_novice executes the proposal</step>
    </mainSequence>
</useCase>
