<useCase id="UC1">
    <name>User Log-out</name>
    <actors>
        <actor>Logged-in User</actor>
    </actors>
    <mainSequence>
        <step id="S1">Logged-in User clicks on "log-out"</step>
        <step id="S2">GUI Main Screen redirects to GUI Current User Store</step>
        <step id="S3">GUI Current User Store resets current user data</step>
        <step id="S4">Return OK</step>
        <step id="S5">Redirect to welcome screen</step>
    </mainSequence>
</useCase>
