<useCase id="UC1">
    <name>Find Match</name>
    <actors>
        <actor>User</actor>
    </actors>
    <mainSequence>
        <step id="S1">User initiates match request</step>
        <step id="S2">API Gateway receives the request</step>
        <step id="S3">Controller processes the request</step>
        <step id="S4">Service checks for a match</step>
        <step id="S5">MatchRequests are retrieved for the user</step>
        <step id="S6">Another MatchRequest is retrieved</step>
        <step id="S7">A random question is fetched</step>
        <step id="S8">SessionInfo is created and attached to MatchRequests</step>
        <step id="S9">MatchRequests are updated</step>
        <step id="S10">User's MatchRequest is sent back</step>
        <step id="S11">User switches to a session page</step>
    </mainSequence>
</useCase>
