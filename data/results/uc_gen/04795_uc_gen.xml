<useCase id="UC1">
    <name>User Research and CMS Strategy Development</name>
    <actors>
        <actor>Employee</actor>
        <actor>URT</actor>
        <actor>TT</actor>
        <actor>CMS</actor>
        <actor>Docs</actor>
    </actors>
    <preconditions>
        <precondition>Employee requests CMS Strategy</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">URT defines research objectives</step>
        <step id="S2">URT communicates research needs to TT</step>
        <step id="S3">URT conducts usability testing with CMS</step>
        <step id="S4">CMS provides feedback data to URT</step>
        <step id="S5">URT analyzes user data</step>
        <step id="S6">URT recommends CMS features based on analysis</step>
        <step id="S7">TT evaluates technical feasibility</step>
        <step id="S8">TT presents technical constraints to URT</step>
        <step id="S9">URT adjusts recommendations based on technical input</step>
        <step id="S10">URT documents high-level CMS strategy</step>
        <step id="S11">Docs creates training materials</step>
        <step id="S12">Docs provides CMS strategy documentation to Employee</step>
        <step id="S13">Employee reviews documentation</step>
        <step id="S14">Employee requests clarification if needed</step>
        <step id="S15">URT provides additional details to Employee</step>
        <step id="S16">Employee initiates CMS implementation</step>
        <step id="S17">TT configures CMS according to strategy</step>
        <step id="S18">CMS confirms completion of configuration</step>
        <step id="S19">TT notifies Employee of CMS setup completion</step>
    </mainSequence>
</useCase>
