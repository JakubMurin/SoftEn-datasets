<useCase id="UC1">
    <name>Accessing Ingest Process Documentation</name>
    <actors>
        <actor>Batch Submitter</actor>
    </actors>
    <preconditions>
        <precondition>Batch Submitter is authenticated</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">Batch Submitter navigates to EDRMS Documentation Section</step>
        <step id="S2">Website requests authentication</step>
        <step id="S3">Authentication Service authenticates user</step>
        <step id="S4">Batch Submitter provides credentials</step>
        <step id="S5">Website receives authentication</step>
        <step id="S6">Website requests Ingest Process Docs</step>
        <step id="S7">Documentation Server retrieves documentation</step>
        <step id="S8">Ingest Process Documentation is returned</step>
        <step id="S9">Documentation Server sends documentation link</step>
        <step id="S10">Website displays documentation link</step>
        <step id="S11">Batch Submitter clicks documentation link</step>
        <step id="S12">Website redirects to documentation</step>
        <step id="S13">Ingest Process Documentation is displayed</step>
    </mainSequence>
</useCase>
