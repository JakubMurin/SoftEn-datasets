<useCase id="UniqueATM">
    <name>ATM Transaction</name>
    <actors>
        <actor>Customer</actor>
        <actor>ATM Machine</actor>
        <actor>Bank System</actor>
    </actors>
    <mainSequence>
        <step id="S1">Insert Card</step>
        <step id="S2">Enter PIN</step>
        <step id="S3">Verify PIN</step>
        <step id="S4">PIN Verified</step>
        <step id="S5">Select Balance Inquiry</step>
        <step id="S6">Request Balance</step>
        <step id="S7">Balance Data</step>
        <step id="S8">Display Balance</step>
        <step id="S9">Select Withdrawal</step>
        <step id="S10">Request Withdrawal</step>
        <step id="S11">Validate Funds</step>
        <step id="S12">Enter Amount</step>
        <step id="S13">Confirm Withdrawal</step>
        <step id="S14">Process Withdrawal</step>
        <step id="S15">Withdrawal Approved</step>
        <step id="S16">Dispense Cash</step>
        <step id="S17">Display Insufficient Funds</step>
        <step id="S18">Select Cancel</step>
        <step id="S19">Transaction Canceled</step>
        <step id="S20">Remove Card</step>
    </mainSequence>
</useCase>
