<useCase id="UC1">
    <name>Complete Data Coordination</name>
    <actors>
        <actor>User</actor>
        <actor>Data Coordinator</actor>
    </actors>
    <trigger>UseCase</trigger>
    <mainSequence>
        <step id="S1">User initiates the data coordination use case</step>
        <step id="S2">User selects data to coordinate</step>
        <step id="S3">User submits the data for coordination</step>
        <step id="S4">Data Coordinator processes the submitted data</step>
        <step id="S5">Data coordination process is completed</step>
    </mainSequence>
</useCase>
