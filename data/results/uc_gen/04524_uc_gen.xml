<useCase id="1">
    <name>Execute Forum Proposal</name>
    <actors>
        <actor>Alice_novice</actor>
        <actor>Forum</actor>
    </actors>
    <mainSequence>
        <step id="S1">Alice_novice creates grants treasury</step>
        <step id="S2">Forum provides treasury address</step>
        <step id="S3">Alice_novice creates forum proposal</step>
        <step id="S4">Forum lists proposals</step>
        <step id="S5">Alice_novice gets list of proposals</step>
        <step id="S6">Alice_novice gives positive/negative feedback on proposals</step>
        <step id="S7">Alice_novice executes proposal</step>
    </mainSequence>
</useCase>
