<useCase id="UC001">
    <name>Register</name>
    <actors>
        <actor>Client</actor>
        <actor>registrationPage</actor>
        <actor>RegisterActivity</actor>
        <actor>users</actor>
    </actors>
    <mainSequence>
        <step id="S1">Client submits a registration form</step>
        <step id="S2">registrationPage sends registration request to RegisterActivity</step>
        <step id="S3">RegisterActivity saves new user to users</step>
    </mainSequence>
</useCase>
