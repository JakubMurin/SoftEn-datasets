<useCase id="UC1">
    <name>Display Transactions in Charts</name>
    <actors>
        <actor>Logged-in User</actor>
        <actor>:GUI Financial Accounts Overview</actor>
        <actor>:GUI Fin. Acc. Details View</actor>
        <actor>:Server</actor>
        <actor>:Database</actor>
    </actors>
    <mainSequence>
        <step id="S1">Logged-in User selects a financial account</step>
        <step id="S2">:GUI Financial Accounts Overview opens</step>
        <step id="S3">:GUI Fin. Acc. Details View opens</step>
        <step id="S4">:Server gets all data of financial account (id)</step>
        <step id="S5">:Database selects all data of financial account (id)</step>
        <step id="S6">Return full data of financial account</step>
        <step id="S7">Display transactions in charts in tab "stats"</step>
    </mainSequence>
</useCase>
