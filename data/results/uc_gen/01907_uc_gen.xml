<useCase id="UC1">
    <name>Perform User Login</name>
    <actors>
        <actor>User</actor>
        <actor>HelloController</actor>
        <actor>Bankomat</actor>
        <actor>SignIn_UpController</actor>
    </actors>
    <mainSequence>
        <step id="S1">User onAcceptButton(ActionEvent) triggers login process.</step>
        <step id="S2">HelloController creates a Bankomat instance.</step>
        <step id="S3">HelloController initializes a new Bankomat with specific details.</step>
        <step id="S4">HelloController adds a new UserAdmin to the Bankomat.</step>
        <step id="S5">HelloController sets the Bankomat in SignIn_UpController.</step>
        <step id="S6">SignIn_UpController displays the Bankomat.</step>
        <step id="S7">HelloController closes the process.</step>
    </mainSequence>
</useCase>
