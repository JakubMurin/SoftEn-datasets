<useCase id="UC1">
    <name>Upload Object with Quota Validation</name>
    <actors>
        <actor>Client</actor>
        <actor>RPC Server</actor>
        <actor>Object Server</actor>
    </actors>
    <mainSequence>
        <step id="S1">Client requests to create object upload</step>
        <step id="S2">RPC Server receives create_object_upload request</step>
        <step id="S3">RPC Server forwards request to Object Server</step>
        <step id="S4">Object Server checks quota</step>
    </mainSequence>
</useCase>
