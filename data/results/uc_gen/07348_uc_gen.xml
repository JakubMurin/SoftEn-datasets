<useCase id="UC1">
    <name>Complete Technical Report</name>
    <actors>
        <actor>User</actor>
        <actor>TicketTransitions</actor>
        <actor>ClientRestService</actor>
        <actor>ProfessionalTicket</actor>
        <actor>TechReport</actor>
    </actors>
    <preconditions>
        <precondition>User initiates completion of technical report.</precondition>
    </preconditions>
    <successEndConditions>
        <condition>Technical report is successfully completed and saved.</condition>
    </successEndConditions>
    <trigger>User completes the technical report.</trigger>
