        <step id="S8">Set report quality.</step>
        <step id="S9">Set report completed time.</step>
        <step id="S10">Set ERP details.</step>
        <step id="S11">Save ticket with status TECH_COMPLETED.</step>
        <step id="S12">Create or update DICOM study.</step>
        <step id="S13">Create or update professional ticket.</step>
        <step id="S14">Generate TRERP response letter.</step>
    </mainSequence>
</useCase>
