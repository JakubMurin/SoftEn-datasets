<useCase id="UC1">
    <name>Import Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File</step>
        <step id="S2">JsonImporterUI creates</step>
        <step id="S3">JsonImporterUI asks user information</step>
        <step id="S4">WarehouseEmployee provides Filename</step>
        <step id="S5">JsonImporterUI activates</step>
        <step id="S6">JsonImporterController creates</step>
        <step id="S7">JsonImporterUI deactivates</step>
        <step id="S8">WarehouseEmployee provides Filename</step>
        <step id="S9">JsonImporterUI activates</step>
        <step id="S10">JsonImporter creates</step>
        <step id="S11">JsonImporter imports the Json File</step>
        <step id="S12">JsonImporter parses the warehouse from the file</step>
        <step id="S13">WarehouseBuilder creates a warehouse</step>
        <step id="S14">Warehouse is created</step>
        <step id="S15">WarehouseBuilder saves the warehouse</step>
        <step id="S16">AisleBuilder creates an aisle</step>
        <step id="S17">Aisle is created</step>
        <step id="S18">AisleBuilder saves the aisle</step>
        <step id="S19">SectionBuilder creates a section</step>
        <step id="S20">Section is created</step>
        <step id="S21">SectionBuilder saves the section</step>
        <step id="S22">ShelfBuilder creates a shelf</step>
        <step id="S23">Shelf is created</step>
        <step id="S24">ShelfBuilder saves the shelf</step>
        <step id="S25">AGVDockBuilder creates an AGV Dock</step>
        <step id="S26">AGV Dock is created</step>
        <step id="S27">AGVDockBuilder saves the AGV Dock</step>
        <step id="S28">JsonImporter deactivates</step>
        <step id="S29">JsonImporterController deactivates</step>
        <step id="S30">JsonImporterUI displays user information</step>
    </mainSequence>
</useCase>
