<useCase id="UC1">
    <name>Import Json File</name>
    <actors>
        <actor>WarehouseEmployee</actor>
    </actors>
    <mainSequence>
        <step id="S1">WarehouseEmployee imports a Json File.</step>
        <step id="S2">JsonImporterUI creates.</step>
        <step id="S3">JsonImporterUI asks user for information.</step>
        <step id="S4">WarehouseEmployee provides the filename.</step>
        <step id="S5">JsonImporterController creates.</step>
        <step id="S6">JsonImporterController calls 'jsonImporter' with filename.</step>
        <step id="S7">JsonImporter creates.</step>
        <step id="S8">JsonImporter imports the warehouse by parsing the plant.</step>
        <step id="S9">WarehouseBuilder creates a warehouse.</step>
        <step id="S10">WarehouseBuilder creates the warehouse.</step>
        <step id="S11">WarehouseBuilder saves the warehouse to the repository.</step>
        <step id="S12">AisleBuilder creates an aisle.</step>
        <step id="S13">AisleBuilder creates the aisle.</step>
        <step id="S14">AisleBuilder saves the aisle to the repository.</step>
        <step id="S15">RowBuilder creates a section.</step>
        <step id="S16">RowBuilder creates the section.</step>
        <step id="S17">RowBuilder saves the section to the repository.</step>
        <step id="S18">ShelfBuilder creates a shelf.</step>
        <step id="S19">ShelfBuilder creates the shelf.</step>
        <step id="S20">ShelfBuilder saves the shelf to the repository.</step>
        <step id="S21">AGVDockBuilder creates an AGV dock.</step>
        <step id="S22">AGVDockBuilder creates the AGV dock.</step>
        <step id="S23">AGVDockBuilder saves the AGV dock to the repository.</step>
    </mainSequence>
</useCase>
