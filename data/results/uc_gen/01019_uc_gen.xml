<useCase id="UC1">
    <name>Scan Proxy Repo</name>
    <actors>
        <actor>ChangeListener</actor>
    </actors>
    <mainSequence>
        <step id="S1">ProxyRepoFileScanner scans the repository</step>
        <step id="S2">ProxyRepoFileScanner adds a new source</step>
        <step id="S3">ProxyRepoSources adds a new record for the source in the database</step>
        <step id="S4">ChangeListener is notified of the change</step>
    </mainSequence>
</useCase>
