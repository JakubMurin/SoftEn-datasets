<useCase id="UC1">
    <name>Add a new other service</name>
    <actors>
        <actor>Admin</actor>
    </actors>
    <mainSequence>
        <step id="S1">Admin requests to add a new other service</step>
        <step id="S2">UI displays add service form</step>
        <step id="S3">Admin submits new service data</step>
        <step id="S4">Controller adds other service</step>
        <step id="S5">Repository saves new service</step>
        <step id="S6">OtherServiceModel creates new service</step>
        <step id="S7">DB inserts new service into database</step>
        <step id="S8">Repository receives new service added</step>
        <step id="S9">Controller sends 200 OK response</step>
        <step id="S10">UI displays success message</step>
    </mainSequence>
</useCase>
