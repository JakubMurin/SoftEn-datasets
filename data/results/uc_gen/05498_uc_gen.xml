<useCase id="UC001">
    <name>Consultation Process</name>
    <actors>
        <actor>Cliente</actor>
        <actor>Secretaria</actor>
        <actor>Veterinario</actor>
    </actors>
    <mainSequence>
        <step id="S1">Cliente marcar consulta.</step>
        <step id="S2">Secretaria confirma consulta.</step>
        <step id="S3">Cliente informa dados pessoais.</step>
        <step id="S4">Secretaria atualiza cadastro do cliente.</step>
        <step id="S5">Secretaria confirma atualização do cadastro.</step>
        <step id="S6">Cliente informa sintomas ao Veterinario.</step>
        <step id="S7">Veterinario registra os sintomas.</step>
        <step id="S8">Veterinario fornece diagnóstico e plano de tratamento ao Cliente.</step>
        <step id="S9">Veterinario marca exames para realizar.</step>
        <step id="S10">Cliente é solicitar exames.</step>
        <step id="S11">Veterinario finaliza tratamento.</step>
        <step id="S12">Cliente retorna para consulta com o Veterinario.</step>
        <step id="S13">Veterinario registra pedido de exames para o Cliente.</step>
        <step id="S14">Cliente recebe resultados no histórico de consultas.</step>
        <step id="S15">Veterinario fornece receitas ao Cliente.</step>
    </mainSequence>
</useCase>
