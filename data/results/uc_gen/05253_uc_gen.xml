<useCase id="UC001">
    <name>Registration</name>
    <actors>
        <actor>User</actor>
        <actor>FrontEnd</actor>
        <actor>MD</actor>
    </actors>
    <preconditions>
        <precondition>User requests registration</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">FrontEnd shows registration form to User</step>
        <step id="S2">User fills in registration form</step>
        <step id="S3">FrontEnd shows Consent form to User</step>
        <step id="S4">User accepts Consent</step>
        <step id="S5">FrontEnd validates form</step>
        <step id="S6">FrontEnd posts registration data to MD</step>
        <step id="S7">MD resolves registration with success</step>
        <step id="S8">FrontEnd shows success message to User</step>
    </mainSequence>
</useCase>
