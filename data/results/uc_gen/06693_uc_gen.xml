<useCase id="UC001">
    <name>Add a New Other Service</name>
    <actors>
        <actor>Admin</actor>
    </actors>
    <mainSequence>
        <step id="S1">Admin requests to add a new other service.</step>
        <step id="S2">UI displays the add service form.</step>
        <step id="S3">Admin submits new service data.</step>
        <step id="S4">System adds the new service successfully.</step>
        <step id="S5">UI displays the success message.</step>
    </mainSequence>
</useCase>
