<useCase id="UC1">
    <name>Remove Project</name>
    <actors>
        <actor>Admin</actor>
    </actors>
    <preconditions>
        <precondition>Admin is authenticated</precondition>
        <precondition>Project exists</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">Admin initiates removal of a project</step>
        <step id="S2">System validates the member</step>
        <step id="S3">System validates the admin</step>
        <step id="S4">System validates the project</step>
        <step id="S5">System removes the project</step>
        <step id="S6">System notifies Admin that the project has been removed</step>
    </mainSequence>
</useCase>
