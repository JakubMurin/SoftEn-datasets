<useCase id="UC1">
    <name>Manage Applications</name>
    <actors>
        <actor>Developer</actor>
    </actors>
    <mainSequence>
        <step id="S1">Developer sends request to fleet-service-inc</step>
        <step id="S2">fleet-service-inc forwards request to Application Stack</step>
        <step id="S3">Application Stack executes sails.helpers.incService()</step>
    </mainSequence>
</useCase>
