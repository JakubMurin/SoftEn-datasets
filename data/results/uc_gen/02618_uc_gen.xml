<useCase id="UC1">
	<name>Point Calculation Process</name>
	<actors>
		<actor>User</actor>
	</actors>
	<mainSequence>
		<step id="S1">User moves map</step>
		<step id="S2">Calculate area in backend</step>
		<step id="S3">Add calculated area to split queue</step>
		<step id="S4">Retrieve depth in area</step>
		<step id="S5">Request Points in depth</step>
		<step id="S6">Get points in area</step>
		<step id="S7">Request not calculated points</step>
		<step id="S8">Calculate and update points</step>
		<step id="S9">Insert calculated points with undefined depth</step>
		<step id="S10">Create and show overlay with updated points</step>
	</mainSequence>
</useCase>
