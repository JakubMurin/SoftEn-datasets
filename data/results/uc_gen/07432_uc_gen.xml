<useCase id="UC1">
    <name>Manage Customer Orders</name>
    <actors>
        <actor>Waiter</actor>
        <actor>Cook</actor>
    </actors>
    <mainSequence>
        <step id="S1">Waiter enters customer order</step>
        <step id="S2">System notifies Cook of new order completion notification</step>
        <step id="S3">Cook acknowledges receipt to System</step>
        <step id="S4">System confirms order in preparation to Waiter</step>
    </mainSequence>
</useCase>
