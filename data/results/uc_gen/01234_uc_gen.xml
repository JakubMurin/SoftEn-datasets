<useCase id="UC1">
	<name>Manage SABR</name>
	<actors>
		<actor>DevOpsEngineer</actor>
	</actors>
	<preconditions>
		<precondition>DevOpsEngineer is authenticated</precondition>
	</preconditions>
	<mainSequence>
		<step id="S1">DevOpsEngineer requests to manage SABR</step>
		<step id="S2">"Sentient Agent Bundle Manager" lists SABR using bin command</step>
		<step id="S3">bin sends a REST request to sabundle/list endpoint</step>
	</mainSequence>
</useCase>
