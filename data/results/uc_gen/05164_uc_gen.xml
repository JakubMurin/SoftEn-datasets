<useCase id="UC1">
    <name>Manage Builds</name>
    <actors>
        <actor>DevSecOpsManager</actor>
    </actors>
    <mainSequence>
        <step id="S1">List Builds</step>
        <step id="S2">DevSecOpsManager lists builds</step>
        <step id="S3">Provisioner receives list request</step>
        <step id="S4">Provisioner lists builds</step>
        <step id="S5">Provisioner sends build list to EventBus</step>
        <step id="S6">EventBus receives build list</step>
    </mainSequence>
</useCase>
