<useCase id="UC001">
    <name>Make teleport gate result</name>
    <actors>
        <actor>Tester</actor>
        <actor>recipe</actor>
        <actor>s</actor>
        <actor>inventory</actor>
        <actor>gate1</actor>
        <actor>gate2</actor>
    </actors>
    <mainSequence>
        <step id="S1">Tester makes result with s.</step>
        <step id="S2">recipe creates TeleportGateItem with ID.</step>
        
        <step id="S3">recipe inserts gate1 into inventory.</step>
        <step id="S4">inventory confirms insertion of gate1.</step>
        
        <step id="S5">recipe inserts gate2 into inventory.</step>
        <step id="S6">inventory confirms insertion of gate2.</step>
        
        <step id="S7">recipe informs Tester of completion.</step>
    </mainSequence>
</useCase>
