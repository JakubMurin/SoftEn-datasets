<useCase id="UC1">
    <name>Turn on LED</name>
    <actors>
        <actor>Led</actor>
    </actors>
    <mainSequence>
        <step id="S1">Arduino sets up the system</step>
        <step id="S2">Main initializes the FSM context</step>
        <step id="S3">Main transitions to OnState</step>
        <step id="S4">OnState executes onEnter()</step>
        <step id="S5">OnState turns on the LED</step>
    </mainSequence>
</useCase>
