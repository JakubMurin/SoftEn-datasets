<useCase id="UC1">
    <name>Check Assigned Tasks</name>
    <actors>
        <actor>Collaborator</actor>
    </actors>
    <mainSequence>
        <step id="S1">Collaborator asks to check assigned tasks.</step>
        <step id="S2">GUI gets assigned entries.</step>
        <step id="S3">AgendaController retrieves instance from Repositories.</step>
        <step id="S4">AgendaController retrieves agenda from Repositories.</step>
        <step id="S5">AgendaController gets assigned entries from Agenda.</step>
        <step id="S6">GUI displays returned entries.</step>
        <step id="S7">Collaborator asks to filter by date or status.</step>
        <step id="S8">GUI filters entries by date and status.</step>
        <step id="S9">AgendaController filters entries by date and status.</step>
        <step id="S10">GUI displays updated entries.</step>
    </mainSequence>
</useCase>
