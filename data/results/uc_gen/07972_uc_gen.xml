<useCase id="UC001">
    <name>Register User</name>
    <actors>
        <actor>User</actor>
    </actors>
    <preconditions>
        <precondition>User is not registered in the system.</precondition>
    </preconditions>
    <mainSequence>
        <step id="S1">User requests to register.</step>
        <step id="S2">FrontEnd shows registration form to User.</step>
        <step id="S3">User fills in registration form.</step>
        <step id="S4">FrontEnd shows consent form to User.</step>
        <step id="S5">User accepts consent.</step>
        <step id="S6">FrontEnd validates form.</step>
        <step id="S7">FrontEnd posts registration to MD.</step>
        <step id="S8">FrontEnd resolves success from MD.</step>
        <step id="S9">FrontEnd shows success message to User.</step>
    </mainSequence>
</useCase>
