<useCase id="UC01">
   <name>Make teleport gate result</name>
   <actors>
      <actor>Tester</actor>
      <actor>recipe</actor>
      <actor>s</actor>
      <actor>inventory</actor>
      <actor>gate1</actor>
      <actor>gate2</actor>
   </actors>
   <mainSequence>
      <step id="S1">Tester makes the result with s.</step>
      <step id="S2">TeleportGateItem(id) is created for gate1.</step>
      <step id="S3">TeleportGateItem(id) is created for gate2.</step>
      <step id="S4">gate1 is inserted into the inventory.</step>
      <step id="S5">gate2 is inserted into the inventory.</step>
   </mainSequence>
</useCase>
