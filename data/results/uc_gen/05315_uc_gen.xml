<useCase id="UC001">
  <name>Negotiation Pattern Sequence</name>
  <actors>
    <actor>Acquirer</actor>
    <actor>Provider</actor>
  </actors>
  <mainSequence>
    <step id="S1">Acquirer makes an initial OFFER</step>
    <step id="S2">Acquirer requests further information from Provider</step>
    <step id="S3">Provider responds with a counter OFFER</step>
    <step id="S4">Acquirer makes another REQUEST</step>
    <step id="S5">Provider provides the requested information</step>
    <step id="S6">Acquirer acknowledges the received information</step>
  </mainSequence>
</useCase>
