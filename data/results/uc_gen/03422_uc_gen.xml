<useCase id="UC1">
    <name>Delete a photo</name>
    <actors>
        <actor>Admin</actor>
    </actors>
    <mainSequence>
        <step id="S1">Admin deletes a photo</step>
        <step id="S2">Frontend requests to delete the photo</step>
        <step id="S3">Backend queries the Database to delete the photo</step>
        <step id="S4">Database returns operation success</step>
        <step id="S5">Backend returns operation success</step>
        <step id="S6">Frontend informs Admin about the operation success</step>
    </mainSequence>
</useCase>
