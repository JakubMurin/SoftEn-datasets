<useCase id="D1">
    <name>Dynamic-Map</name>
    <actors>
        <actor>actorName</actor>
    </actors>
    <trigger>A User triggers dynamic map updates.</trigger>
    <mainSequence>
        <step id="S1">actorName triggers "adc-dynamic-update()" from CLI interface.</step>
        <step id="S2">CLI forwards request to adc: "dynamic/update()".</step>
        <step id="S3">actorName sends "dynamic/update()" request to Web.</step>
        <step id="S4">Web forwards the request to adc: "dynamic/update()".</step>
    </mainSequence>
</useCase>
