<useCase id="UC1">
    <name>User Registration</name>
    <actors>
        <actor>User</actor>
        <actor>FrontEnd</actor>
        <actor>MD</actor>
    </actors>
    <preconditions>
        <precondition>User wants to register in the system.</precondition>
    </preconditions>
    <successEndConditions>
        <condition>User successfully registers in the system.</condition>
    </successEndConditions>
    <trigger>User requests registration.</trigger>
    <mainSequence>
        <step id="S1">User requests registration.</step>
        <step id="S2">FrontEnd shows registration form.</step>
        <step id="S3">User fills in registration form.</step>
        <step id="S4">FrontEnd shows Consent form.</step>
        <step id="S5">User accepts Consent.</step>
        <step id="S6">FrontEnd validates form.</step>
        <step id="S7">FrontEnd post(register) to MD.</step>
        <step id="S8">MD resolves with success to FrontEnd.</step>
        <step id="S9">FrontEnd shows success message to User.</step>
    </mainSequence>
</useCase>
